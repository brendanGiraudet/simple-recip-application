    "FileSettings": {
        "MaxAllowedSize": 512000
    },
    "OpenApisettings": {
        "ApiKey": "",
        "ChatCompletionUrl": "https://api.openai.com/v1/chat/completions",
        "RetryCount": 3,
        "RetryDelaySeconds": 2,
        "MaxToken": 1000
    },
    "OAuthGoogleSettings": {
        "ClientId": "",
        "ClientSecret": ""
    }
